{
    "name": "cumulati/laravel-facade-generator",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cumulati\\LaravelFacadeGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Stubs\\Class\\": "class_stubs/",
            "Stubs\\Facade\\": "facade_stubs/"
        }
    },
    "authors": [
        {
            "name": "Patrick McCarren",
            "email": "patrick@cumulative.io"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^8.1",
        "roave/better-reflection": "^6.0"
    },
    "require-dev": {
        "pestphp/pest": "^3.2",
        "symfony/var-dumper": "^7.0",
        "symfony/finder": "^7.0"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    }
}
